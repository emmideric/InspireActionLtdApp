<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.inspireapp">

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="online.inspiredactionlimited.inspiredactionltdapp.AppointmentActivity"
            android:parentActivityName="online.inspiredactionlimited.inspiredactionltdapp.MainActivity" />
        <activity android:name="online.inspiredactionlimited.inspiredactionltdapp.MainActivity"></activity>
        <activity android:name="online.inspiredactionlimited.inspiredactionltdapp.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>